Items = [...state.items];\r\n          updatedItems[indexofUpdatedItem] = updatedItem;\r\n        }\r\n        return { items: updatedItems, totalAmount: updatedAmount };\r\n      }\r\n      break;\r\n\r\n    default:\r\n      return { items: [], totalAmount: 0 };\r\n  }\r\n};\r\n\r\nconst CartProvider = (props) => {\r\n  const [cartState, dispatchCartAction] = useReducer(CartReducer, {\r\n    items: [],\r\n    totalAmount: 0,\r\n  });\r\n  const addItemToCart = (item) => {\r\n    dispatchCartAction({ type: \"ADD\", newItem: item });\r\n  };\r\n  const removeItemToCart = (id) => {\r\n    dispatchCartAction({ type: \"REMOVE\", id: id });\r\n  };\r\n  const cartContext = {\r\n    items: cartState.items,\r\n    totalAmount: cartState.totalAmount,\r\n    addItem: addItemToCart,\r\n    removeItem: removeItemToCart,\r\n  };\r\n  return (\r\n    <CartContext.Provider value={cartContext}>\r\n      {props.children}\r\n    </CartContext.Provider>\r\n  );\r\n};\r\n\r\nexport default CartProvider;\r\n","C:\\Users\\najeh\\Projects\\React\\FoodOrder\\src\\components\\context\\cart-context.js",[],"C:\\Users\\najeh\\Projects\\React\\FoodOrder\\src\\components\\Cart\\CartItem.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":1,"column":10,"nodeType":"86","messageId":"87","endLine":1,"endColumn":18},{"ruleId":"84","severity":1,"message":"88","line":1,"column":18,"nodeType":"86","messageId":"87","endLine":1,"endColumn":26},{"ruleId":"84","severity":1,"message":"89","line":1,"column":8,"nodeType":"86","messageId":"87","endLine":1,"endColumn":13},{"ruleId":"90","severity":1,"message":"91","line":50,"column":7,"nodeType":"92","messageId":"93","endLine":50,"endColumn":13},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'Fragment' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","'react' is defined but never used.","no-unreachable","Unreachable cod